            }
            if (networkTypeTmp == 'limited_service' || networkTypeTmp == 'no_service') {
                $("#statusBar #networkType").attr("trans", "network_type_" + networkTypeTmp);
                return $.i18n.prop("network_type_" + networkTypeTmp);
            } else {
