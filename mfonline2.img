	 * @event deleteSelectClickHandler
	 */
	deleteSelectClickHandler = function(){
		showConfirm("confirm_data_delete", function() {
            showLoading('deleting');
			var items = getIdsBySelectedIds();
			service.deleteMessage({
				ids: items.ids
			}, function(data){
